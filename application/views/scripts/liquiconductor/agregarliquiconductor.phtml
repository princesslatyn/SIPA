      <select class="form-control" id="paga" name="paga" style="cursor:pointer;">
      <option>Trámite</option>  
      </select> 
      </div>    
      </div>
       <div class="col-lg-6">    
      <div class="form-group">
      <label for="input" class="">Codigo Práctica</label>           
      <select class="form-control" id="paga" name="paga" style="cursor:pointer;">
      <option>Pago Realizado</option>
      </select>
      </div>    
      </div> 
      </div> 
      
</form>
 
</div>
</div>    
   <div class="row">
   <div class=" text-center">
       <input type="button" id="agregar" value="Guardar"  class="btn btn-default"  />
